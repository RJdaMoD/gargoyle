config adblock 'config'
	option enabled '0'
	option trans '0'
	option exempt '0'
	option exstart '192.168.1.1'
	option exend '192.168.1.255'
	option endpoint4 '0.0.0.0'
	option endpoint6 '::'
	list adblock_src 'https://winhelp2002.mvps.org/hosts.txt'
	list adblock_src 'https://adaway.org/hosts.txt'

